<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>com.github.wujinpeng.intellijplatformpluginclickablehyperlink</id>
    <name>IntelliJ Platform Plugin intellij-platform-plugin-clickable-hyperlink</name>
    <vendor>wu-jinpeng</vendor>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.github.wujinpeng.intellijplatformpluginclickablehyperlink.services.MyApplicationService"/>
        <projectService serviceImplementation="com.github.wujinpeng.intellijplatformpluginclickablehyperlink.services.MyProjectService"/>
    </extensions>

    <applicationListeners>
        <listener class="com.github.wujinpeng.intellijplatformpluginclickablehyperlink.listeners.MyProjectManagerListener"
                  topic="com.intellij.openapi.project.ProjectManagerListener"/>
    </applicationListeners>
</idea-plugin>
